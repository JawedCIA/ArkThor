@{
    ViewData["Title"] = "UnderDevelopment";

    Layout = "~/Views/Shared/_Layout.cshtml";
    <style>
        /* CSS styles for the message container */
        .message-container {
            width: 400px;
            margin: 100px auto;
            padding: 20px;
            background-color: #f5f5f5;
            text-align: center;
            border: 1px solid #ccc;
            border-radius: 4px;
        }

        /* CSS styles for the message text */
        .message-text {
            font-size: 24px;
            color: #333;
        }
    </style>
}

@section scripts {
  


    <script type="text/javascript">
        'use strict';       
        
       

    </script>

}

<div class="content-wrapper" id="divContentWrapperRequestDetails">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>UnderDevelopment</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="~/Home">Home</a></li>
                        <li class="breadcrumb-item active">UnderDevelopment</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        @*<div class="card card-info collapsed-card">*@
        <div class="card card-info">
            <div class="card-header">
                <h3 class="card-title">Page Under Development</h3>
                <div class="card-tools">

                    <button type="button" class="btn btn-tool" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                        <i class="fas fa-minus"></i>
                    </button>
                </div>
            </div>
            <!-- <form class="form-horizontal">-->
            <div class="card-body">
                <div class="message-container">
                    <h1 class="message-text">Page Under Development</h1>
                    <p class="message-text">We apologize for the inconvenience. This page is currently under development.</p>
                </div>

            </div>
         

        </div>
        <!-- /.card -->
    </section>
  
</div>




