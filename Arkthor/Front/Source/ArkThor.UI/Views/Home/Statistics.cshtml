
@{
    ViewData["Title"] = "Statistics";
    Layout = "~/Views/Shared/_Layout.cshtml";

     
        <!-- daterange picker -->
    <link rel="stylesheet" href="~/theme/plugins/daterangepicker/daterangepicker.css">
    <!-- Tempusdominus Bbootstrap 4 -->
    <link rel="stylesheet" href="~/theme/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
   
}

@section scripts {
    <!-- FLOT CHARTS -->
<!--<script src="~/theme/plugins/flot/jquery.flot.js"></script>-->
 <script src="~/theme/MD/js/CSCD-Charts.js"></script>

 <!-- jQuery -->
<script src="~/theme/plugins/jquery/jquery.min.js"></script>
<!-- jQuery UI -->
<script src="~/theme/plugins/jquery-ui/jquery-ui.min.js"></script>
<!-- Bootstrap 4 -->
<script src="~/theme/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>

<!-- InputMask -->
<script src="~/theme/plugins/moment/moment.min.js"></script>
<script src="~/theme/plugins/inputmask/min/jquery.inputmask.bundle.min.js"></script>
<!-- Tempusdominus Bootstrap 4 -->
<script src="~/theme/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>

<!-- FLOT CHARTS -->
<script src="~/theme/plugins/flot/jquery.flot.js"></script>
<!-- FLOT RESIZE PLUGIN - allows the chart to redraw when the window is resized -->
<script src="~/theme/plugins/flot-old/jquery.flot.resize.min.js"></script>
<!-- FLOT PIE PLUGIN - also used to draw donut charts -->
<script src="~/theme/plugins/flot-old/jquery.flot.pie.min.js"></script>
    <!-- ChartJS -->
    <script src="~/theme/plugins/chart.js/Chart.min.js"></script>
   <!--<script src="~/theme/plugins/chartjs-plugin-datalabels/chartjs-plugin-datalabels.js"></script>-->
    <!-- jQuery Knob -->
    <script src="~/theme/plugins/jquery-knob/jquery.knob.min.js"></script>
    <script src="~/theme/Extra/js/js-country-code.min.js"></script>
    <!-- Sparkline -->
    <!--<script src="~/theme/plugins/sparkline/jquery.sparkline.min.js"></script>-->
    <!-- Page script -->
    <script type="text/javascript">
    
      $(function () {

          //Date range picker
              $('#divStatisticsdateFrom').datetimepicker({
                  format: 'YYYY-MM-DD'//'DD-MM-YYYY'
              });

               $('#divStatisticsdateTo').datetimepicker({
                   format: 'YYYY-MM-DD'
              });
           // drawKnob();
              ShowAllMeasurementGraph();
   
    });
    </script>
}
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper" id="divContentWrapperStatistics">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Statistics</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Statistics </li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">

        <!-- Default box -->
        <div class="card card-info">
            <div class="card-header">
                <h3 class="card-title">
                  <i class="nav-icon fas fa-chart-pie"></i>
                  Activitise - Measurement
                </h3>

                <div class="card-tools">
                  <button type="button" class="btn btn-sm btn-tool" onclick="generatePDFStatistics();">
                            <i class="fas fa-download"></i>
                        </button>
                    <button type="button" class="btn btn-tool" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                        <i class="fas fa-minus"></i>
                    </button>

                </div>
            </div>
    <div class="card-body">
        <!-- Selection Area-->
          <div class="form-group row">

                    <div class="col-md-1">
                        <label >From:</label>
                    </div>
                    <div class="col-md-3">
                        
                        <div class="input-group date" id="divStatisticsdateFrom" data-target-input="nearest">
                            <input type="text" class="form-control datetimepicker-input" data-target="#divStatisticsdateFrom" id="txtStatisticsdateFrom" required />
                            <div class="input-group-append" data-target="#divStatisticsdateFrom" data-toggle="datetimepicker">
                                <div class="input-group-text">
                                    <i class="fa fa-calendar"></i>
                                </div>
                            </div>
                        </div>
                    </div>     

                    <label class="col-md-1">To:</label>
                    <div class="col-md-3">
                        <div class="input-group date" id="divStatisticsdateTo" data-target-input="nearest">
                            <input type="text" class="form-control datetimepicker-input" data-target="#divStatisticsdateTo" id="txtStatisticsdateTo" required />
                            <div class="input-group-append" data-target="#divStatisticsdateTo" data-toggle="datetimepicker">
                                <div class="input-group-text">
                                    <i class="fa fa-calendar"></i>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-3">
                         <button type="button" class="btn btn-primary" onclick="ShowAllMeasurementGraph()"> Show Measurement </button>
                    </div>
                </div>


         <!-- Threat Type and Status Distribution-->
                    <div class="row">
                        <div class="col-md-6">
                            <!-- Type Chart -->
                            <div class="card card-danger card-outline">
                                <div class="card-header"> 
                                    <h3 class="card-title">
                                    <i class="fas fa-chart-pie"></i>
                                        Threat Type Distribution
                                    </h3>

                                    <div class="card-tools">
                                        <button type="button" class="btn btn-tool" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                                            <i class="fas fa-minus"></i>
                                        </button>

                                    </div>
                                </div>
                                <div class="card-body" id="chartBody_typeWiseDistributionChart">
                                   
                                    <div id="donut-chart-Type" style="height: 300px;"></div>
                                   
                                    <!--<canvas id="typeWiseDistributionChart" style="height: 300px;"></canvas>-->
                                </div>
                                <!-- /.card-body-->
                            </div>
                            <!-- /.card -->
                        </div>

                    <div class="col-6 text-center">
                        <div class="card card-primary card-outline">
                            <div class="card-header">
                                <h3 class="card-title">
                                    <i class="fas fa-chart-pie"></i>
                                    File Analyzed rate
                                </h3>

                                <div class="card-tools">
                                    <button type="button" class="btn btn-tool" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                                        <i class="fas fa-minus"></i>
                                    </button>

                                </div>
                            </div>
                            <div class="card-body" id="chartBody_AnalyzedRate">
                                <input type="text" class="knob" value="0" data-width="250" data-height="250" data-fgColor="rgb(0,128,0)" id="inputTxtKnobRate" readonly>

                                <div class="knob-label">Analyzed Rate</div>

                            </div>
                            <!-- /.card-body-->
                        </div>
                        <!-- /.card -->
                    </div>

                    </div> <!--/ Status and Type Row Chart -->
                <!-- Threat Type and Status Distribution-->
                <div class="row">
                    <div class="col-md-6">
                        <!-- Type Chart -->
                        <div class="card card-danger card-outline">
                            <div class="card-header">
                                <h3 class="card-title">
                                    <i class="far fa-chart-bar"></i>
                                    C2 Communication Countries
                                </h3>

                                <div class="card-tools">
                                    <button type="button" class="btn btn-tool" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                                        <i class="fas fa-minus"></i>
                                    </button>

                                </div>
                            </div>
                            <div class="card-body" id="chartBody_CountriesWiseDistributionChart">

                                <canvas id="canvasCountriesWiseDistributionChart" style="height: 300px;"></canvas>

                                <!--<canvas id="typeWiseDistributionChart" style="height: 300px;"></canvas>-->
                            </div>
                            <!-- /.card-body-->
                        </div>
                        <!-- /.card -->
                    </div>
                    <div class="col-md-6">
                        <!-- Type Chart -->
                        <div class="card card-danger card-outline">
                            <div class="card-header">
                                <h3 class="card-title">
                                    <i class="far fa-chart-bar"></i>
                                    C2 Infected Countries
                                </h3>

                                <div class="card-tools">
                                    <button type="button" class="btn btn-tool" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                                        <i class="fas fa-minus"></i>
                                    </button>

                                </div>
                            </div>
                            <div class="card-body" id="chartBody_InfCountriesWiseDistributionChart">

                                <canvas id="canvasInfCountriesWiseDistributionChart" style="height: 300px;"></canvas>

                                <!--<canvas id="typeWiseDistributionChart" style="height: 300px;"></canvas>-->
                            </div>
                            <!-- /.card-body-->
                        </div>
                        <!-- /.card -->
                    </div>
                   
                   
                </div> <!--/ Status and Type Row Chart -->
                <!--File Uploaded -->
                <div class="row">
                    <div class="col-6">
                        <div class="card card-primary card-outline">
                            <div class="card-header">
                                <h3 class="card-title">
                                    <i class="fas fa-chart-pie"></i>
                                    File Status Distribution
                                </h3>

                                <div class="card-tools">
                                    <button type="button" class="btn btn-tool" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                                        <i class="fas fa-minus"></i>
                                    </button>

                                </div>
                            </div>
                            <div class="card-body" id="chartBody_StatusWiseDistributionChart">
                                <div id="donut-chart-Status" style="height: 300px;"></div>
                            </div>
                            <!-- /.card-body-->
                        </div>
                        <!-- /.card -->
                    </div>

                    <div class="col-6">
                        <!-- Release Chart -->
                        <div class="card card-warning card-outline">
                            <div class="card-header">
                                <h3 class="card-title">
                                    <i class="far fa-chart-bar"></i>
                                    File Uploaded and Analyzed Distribution
                                </h3>

                                <div class="card-tools">
                                    <button type="button" class="btn btn-tool" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                                        <i class="fas fa-minus"></i>
                                    </button>

                                </div>
                            </div>
                            <div class="card-body" id="chartBody_UploadedChart">

                                <canvas id="canvasUploadedChart" style="height: 300px;"></canvas>
                            </div>
                            <!-- /.card-body-->
                        </div>
                        <!-- /.card -->
                    </div>
                </div>

                <!--File Analyzed -->
                <div class="row">
                    <div class="col-12">
                        <!-- Release Chart -->
                        <div class="card card-success card-outline">
                            <div class="card-header">
                                <h3 class="card-title">
                                    <i class="far fa-chart-bar"></i>
                                    File Analyses Distribution
                                </h3>

                                <div class="card-tools">
                                    <button type="button" class="btn btn-tool" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                                        <i class="fas fa-minus"></i>
                                    </button>

                                </div>
                            </div>
                            <div class="card-body" id="chartBody_AnalysesChart" >

                                <canvas id="canvasAnalysesChart" style="height: 300px;"></canvas>
                            </div>
                            <!-- /.card-body-->
                        </div>
                        <!-- /.card -->
                    </div>
                </div>




            </div>
		
            
           
         
            <div class="card-footer">
                    ArkThor connecting the dots by providing context on indicators of compromise and tactics, techniques and procedures (TTPs) of threat actors.
            </div>
            <!-- /.card-footer-->
        </div>
        <!-- /.card -->

    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
